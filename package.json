{
  "name": "rack-label-tool",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/PrimeGoose/label_maker_tool",
    "branch": "Angular",
    "path": "label_maker_tool"
  }
  ,
  "scripts":{
    "ng": "ng",
    "ngh": "ngh",
    "start": "ng serve --host='0.0.0.0'",
    "build": "ng build",
    "prod": "ng build --configuration production  --progress --output-hashing=none --vendor-chunk=true --named-chunks=false --build-optimizer=true --optimization=true --source-map=false --watch=false --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-gh-pages": "ng build --configuration production --base-href https://PrimeGoose.github.io/label_maker_tool/",
    "deploy-gh-pages": "ngh --dir=dist/label-maker-tool"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "~15.2.9",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "~15.2.9",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.2",
    "@angular/cli": "~16.2.2",
    "@angular/compiler-cli": "^16.2.0",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jasmine": "~4.3.0",
    "angular-cli-ghpages": "^1.0.7",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-tailwind": "^4.0.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "~5.1.3"
  }
}
